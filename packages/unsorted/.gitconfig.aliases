[alias]
  #
  l = ! git log --oneline --color
  lg = ! git log --oneline --graph --decorate --color

  #
  s = status -s

  #
  cm = commit -m

  #
  co = checkout
  cob = checkout -b

  #
  br = branch
  brd = branch -d

  # Amend the currently staged files to the latest commit
  amend = commit --amend --reuse-message=HEAD

  #
  mnf = merge --no-ff
